{
  "uid": "rag-sem-cache",
  "title": "RAG Semantic Cache",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "panels": [
    {
      "type": "timeseries",
      "title": "Requests per second",
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 8 },
      "targets": [
        {
          "expr": "rate(rag_requests_total[1m])",
          "legendFormat": "rps"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "Prometheus" }
    },
    {
      "type": "timeseries",
      "title": "Cache hit rate",
      "gridPos": { "x": 0, "y": 8, "w": 24, "h": 8 },
      "targets": [
        {
          "expr": "rate(rag_cache_hits_total[1m]) / rate(rag_requests_total[1m])",
          "legendFormat": "hit_rate"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "Prometheus" }
    },
    {
      "type": "timeseries",
      "title": "P95 latency (ms)",
      "gridPos": { "x": 0, "y": 16, "w": 24, "h": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(rag_query_latency_ms_bucket[5m])) by (le))",
          "legendFormat": "p95"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "Prometheus" }
    }
  ]
}
